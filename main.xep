object File {

    let path: string;

    _fun File(path: string): this;

    __builtin fun open(): bool;
    __builtin fun close(): bool;

    fun read(pos: int, size: int, buf: *byte): int;
    fun readall(): *byte;

}

fun main(varargs args: string) {

    let fd: File("/etc/passwd");

    if (!fd->open()) {
        throw Runtime("open failed");
    }

    let srccode: string = fd->readall => valueof;
    println(srccode);

}